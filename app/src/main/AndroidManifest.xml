<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!-- android:name er for den globale Application klassen. -->
    <application
        android:name=".ApplicationClass"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:label="@string/app_name"
        android:icon="@mipmap/addition"
        android:roundIcon="@mipmap/addition_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MattespillForBarn"
        android:configChanges="locale"
        tools:targetApi="31">
        <activity
            android:name=".StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OmActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".PreferanserActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".SpillActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".ResultatActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>